.container {
  width: 100%;
  height: 100%;
}
.video-wrapper {
  width: 100%;
  height: 100%;
  padding-bottom: 64rpx;
  box-sizing: border-box;
  background: #292C30;
}
.control-wrapper {
  position: fixed;
  bottom: 18px;
  left:0;
  display: flex;
  width: 100%;
  box-sizing: border-box;
  flex-direction:row;
  justify-content: space-between;
  padding: 0 42rpx;
}
.control-wrapper .item{
  width: 92rpx;
  height: 92rpx;
}
.disabled {
  filter:brightness(0.4);
}
